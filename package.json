{
  "name": "roman-numeral-converter",
  "version": "1.0.0",
  "description": "A web app to convert numbers to Roman Numerals",
  "main": "server.js",
"scripts": {
  "server": "nodemon server/server.js",
  "client": "npm start --prefix client",
  "dev": "concurrently \"npm run server\" \"npm run client\"",
  "docker-build": "docker build -t roman-numeral-converter .",
  "docker-run": "docker run -p 8080:8080 roman-numeral-converter"
},
  "keywords": ["roman numerals", "converter", "express", "react"],
  "author": "Jerusha Devadason",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  }
}
